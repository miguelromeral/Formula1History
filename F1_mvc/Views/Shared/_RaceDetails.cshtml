@using F1_mvc.Models
@model  Dictionary<string, object>

@foreach (var item in @Model)
{
<tr>
    <td>@item.Key</td>

    @{
        if (item.Value is drivers)
        {
            drivers t = (drivers) item.Value;
            <td>@Html.ActionLink(t.forename +" "+t.surname, "details", "drivers", new { id = (t.driverRef) }, null)</td>
        }
        else
        {
            <td></td>
        }
    }
</tr>
}